<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="menubar_icon">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_35sdp</item>
        <item name="tint">?icon_tint</item>
        <item name="android:layout_margin">@dimen/_5sdp</item>
    </style>

    <style name="menubar_txt">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_35sdp</item>
        <item name="android:textColor">?icon_tint</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:textSize">@dimen/_15sdp</item>
        <item name="android:layout_margin">@dimen/_5sdp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingStart">@dimen/_10sdp</item>
    </style>
</resources>